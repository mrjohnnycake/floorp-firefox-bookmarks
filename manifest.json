{
    "required_api_version": "^2.0.0",
    "name": "Firefox Bookmarks",
    "description": "Search your Firefox bookmarks",
    "developer_name": "Denis Gerber",
    "icon": "images/icon.png",
    "options": {
      "query_debounce": 0.1
    },
    "preferences": [
      {
        "id": "kw",
        "type": "keyword",
        "name": "Firefox Bookmarks",
        "default_value": "f"
      },
      {
        "id": "limit",
        "type": "input",
        "name": "Results number",
        "default_value": "5"
      },
      {
        "id": "path",
        "type": "input",
        "name": "Path to Firefox (requires restart)",
        "default_value": "~/.mozilla/firefox/"
      }
    ]
  }
  
